			<cache-name>OrgIdsMapping</cache-name>
			<scheme-name>orgIdsMapping-distributed</scheme-name>
		</cache-mapping>
        
    </caching-scheme-mapping>

    <caching-schemes>
        <!--cloud-->
        <distributed-scheme>
            <scheme-name>cloud-distributed</scheme-name>
            <service-name>CloudDistributedService</service-name>
            <thread-count>5</thread-count>
            <backing-map-scheme>
                <read-write-backing-map-scheme>
                    <internal-cache-scheme>
                        <local-scheme>
                            <eviction-policy>HYBRID</eviction-policy><!-- three provided strategy LRU,LFU and HYBRID -->
                            <high-units>500</high-units><!-- exceed will be pruning -->
                            <low-units>0</low-units><!-- default,higher than 75% will be pruning -->
                            <expiry-delay>10D</expiry-delay><!-- expire time -->
                        </local-scheme>
                    </internal-cache-scheme>
                    <cachestore-scheme>
                        <class-scheme>
                            <class-name>
                                com.oracle.acs.infra.garadapter.cache.GarInternalJsonCacheLoader
                            </class-name>
                        </class-scheme>
                    </cachestore-scheme>
                    <refresh-ahead-factor>0</refresh-ahead-factor>
                </read-write-backing-map-scheme>
            </backing-map-scheme>
        </distributed-scheme>
        <distributed-scheme>
            <scheme-name>15M-cloud-distributed</scheme-name>
            <service-name>CloudDistributedService</service-name>
            <thread-count>5</thread-count>
            <backing-map-scheme>
                <read-write-backing-map-scheme>
                    <internal-cache-scheme>
                        <local-scheme>
                            <eviction-policy>HYBRID</eviction-policy><!-- three provided strategy LRU,LFU and HYBRID -->
                            <high-units>500</high-units><!-- exceed will be pruning -->
                            <low-units>0</low-units><!-- default,higher than 75% will be pruning -->
                            <expiry-delay>15M</expiry-delay><!-- expire time -->
                        </local-scheme>
                    </internal-cache-scheme>
                    <cachestore-scheme>
                        <class-scheme>
                            <class-name>
                                com.oracle.acs.infra.garadapter.cache.GarInternalJsonCacheLoader
                            </class-name>
                        </class-scheme>
                    </cachestore-scheme>
                    <refresh-ahead-factor>0</refresh-ahead-factor>
                </read-write-backing-map-scheme>
            </backing-map-scheme>
            <autostart>true</autostart>
        </distributed-scheme>
        
        <!--camcom-->
        <replicated-scheme>
            <scheme-name>cam-replicated</scheme-name>
            <service-name>cam-replicated</service-name>
            <backing-map-scheme>
                 <local-scheme>
                       <cachestore-scheme>
                        <class-scheme>
                            <class-name>
                                com.sun.service.omp.am.cacheloader.CamOrganizationLoader
                            </class-name>
                        </class-scheme>
                    </cachestore-scheme>    
               </local-scheme>
            </backing-map-scheme>
            <autostart>true</autostart>
        </replicated-scheme>
        <distributed-scheme>
            <scheme-name>gar-config-distributed</scheme-name>
            <service-name>gar-config-distributed</service-name>
            <thread-count>5</thread-count>
            <backing-map-scheme>
                <read-write-backing-map-scheme>
                    <internal-cache-scheme>
                        <local-scheme>
                            <expiry-delay>0</expiry-delay>
                        </local-scheme>
                    </internal-cache-scheme>
                    <cachestore-scheme>
                        <class-scheme>
                            <class-name>
                                com.oracle.acs.infra.garadapter.cache.GarInternalConfigCacheLoader
                            </class-name>
                        </class-scheme>
                    </cachestore-scheme>
                    <refresh-ahead-factor>0</refresh-ahead-factor>
                    <!-- There is no need to set refresh refactor , config isn't changed often-->
                </read-write-backing-map-scheme>
            </backing-map-scheme>
            <autostart>true</autostart><!-- set auto start true in order that gar config is not changed often -->
        </distributed-scheme>
        <distributed-scheme>
            <scheme-name>8h-misc-distributed</scheme-name>
            <service-name>8h-misc-distributed</service-name>
            <thread-count>5</thread-count>
            <backing-map-scheme>
                <local-scheme>
                    <eviction-policy>HYBRID</eviction-policy>
                    <high-units>500</high-units>
                    <expiry-delay>8h</expiry-delay>
                    <!--flush-delay>1m</flush-delay-->
                    <!--expiry-enabled>true</expiry-enabled-->
                </local-scheme>
            </backing-map-scheme>
            <autostart>true</autostart>
        </distributed-scheme>
        
        <!--tktrpt-->
        <local-scheme>
            <scheme-name>5D-unlimited-backing-map</scheme-name>
            <eviction-policy>HYBRID</eviction-policy>
            <expiry-delay>5D</expiry-delay> 
            <!--flush-delay>1m</flush-delay-->
            <!--expiry-enabled>true</expiry-enabled-->
        </local-scheme>
        <distributed-scheme>
            <scheme-name>CachedCI</scheme-name>
            <service-name>CachedCI</service-name>
            <thread-count>5</thread-count>
            <backing-map-scheme>
                <local-scheme>
                    <eviction-policy>HYBRID</eviction-policy>
                    <expiry-delay>4h</expiry-delay>
                    <!--flush-delay>1m</flush-delay-->
                    <!--expiry-enabled>true</expiry-enabled-->
                    <cachestore-scheme>
                        <class-scheme>
                            <class-name>com.sun.itsp.service.cms.CmsCacheLoader</class-name>
                            <init-params>
                                <init-param>
                                    <param-type>java.lang.String</param-type>
                                    <param-value>{cache-name}</param-value>
                                </init-param>
                                <init-param>
                                    <param-type>com.tangosol.net.BackingMapManagerContext</param-type>
                                    <param-value>{manager-context}</param-value>
                                </init-param>
                            </init-params>
                        </class-scheme>
                    </cachestore-scheme>
                </local-scheme>
            </backing-map-scheme>
            <autostart>true</autostart>
        </distributed-scheme>       
        <distributed-scheme> 
            <scheme-name>briefusers-distributed</scheme-name> 
            <service-name>briefusers-distributed</service-name> 
            <thread-count>5</thread-count>
            <backing-map-scheme> 
                <read-write-backing-map-scheme> 
                    <internal-cache-scheme> 
                        <local-scheme> 
                            <scheme-ref>5D-unlimited-backing-map</scheme-ref> 
                        </local-scheme> 
                    </internal-cache-scheme> 
                    <cachestore-scheme> 
                        <class-scheme> 
                            <class-name> 
                                com.oracle.acs.infra.camadapter.BriefUserCacheLoader 
                            </class-name> 
                            <init-params> 
                                <init-param> 
                                    <param-type>java.lang.String</param-type> 
                                    <param-value>{cache-name}</param-value> 
                                </init-param> 
                                <init-param> 
                                    <param-type>com.tangosol.net.BackingMapManagerContext</param-type> 
                                    <param-value>{manager-context}</param-value> 
                                </init-param> 
                            </init-params> 
                        </class-scheme> 
                    </cachestore-scheme> 
                    <refresh-ahead-factor>0.5</refresh-ahead-factor> 
                </read-write-backing-map-scheme> 
            </backing-map-scheme> 
            <autostart>true</autostart> 
        </distributed-scheme>
        <distributed-scheme>
            <scheme-name>reportgroups-distributed</scheme-name>
            <service-name>reportgroups-distributed</service-name>
            <thread-count>5</thread-count>
            <backing-map-scheme>
                <read-write-backing-map-scheme>
                    <internal-cache-scheme>
                        <local-scheme>
                            <scheme-ref>5D-unlimited-backing-map</scheme-ref>
                        </local-scheme>
                    </internal-cache-scheme>
                    <cachestore-scheme>
                        <class-scheme>
                            <class-name>
                                com.oracle.acs.reporting.ReportGroupCacheLoader
                            </class-name>
                            <init-params>
                                <init-param>
                                    <param-type>java.lang.String</param-type>
                                    <param-value>{cache-name}</param-value>
                                </init-param>
                                <init-param>
                                    <param-type>com.tangosol.net.BackingMapManagerContext</param-type>
                                    <param-value>{manager-context}</param-value>
                                </init-param>
                            </init-params>
                        </class-scheme>
                    </cachestore-scheme>
                    <refresh-ahead-factor>0.5</refresh-ahead-factor>
                </read-write-backing-map-scheme>
            </backing-map-scheme>
            <autostart>true</autostart>
        </distributed-scheme>
        <distributed-scheme>
            <scheme-name>reportobjects-distributed</scheme-name>
            <service-name>reportobjects-distributed</service-name>
            <thread-count>5</thread-count>
            <backing-map-scheme>
                <read-write-backing-map-scheme>
                    <internal-cache-scheme>
                        <local-scheme>
                            <scheme-ref>5D-unlimited-backing-map</scheme-ref>
                        </local-scheme>
                    </internal-cache-scheme>
                    <cachestore-scheme>
                        <class-scheme>
                            <class-name>
                                com.oracle.acs.reporting.ReportObjectCacheLoader
                            </class-name>
                            <init-params>
                                <init-param>
                                    <param-type>java.lang.String</param-type>
                                    <param-value>{cache-name}</param-value>
                                </init-param>
                                <init-param>
                                    <param-type>com.tangosol.net.BackingMapManagerContext</param-type>
                                    <param-value>{manager-context}</param-value>
                                </init-param>
                            </init-params>
                        </class-scheme>
                    </cachestore-scheme>
                    <refresh-ahead-factor>0.5</refresh-ahead-factor>
                </read-write-backing-map-scheme>
            </backing-map-scheme>
            <autostart>true</autostart>
        </distributed-scheme>       
        <distributed-scheme>
            <scheme-name>subscriptionGroups-distributed</scheme-name>
            <service-name>subscriptionGroups-distributed</service-name>
            <thread-count>5</thread-count>
            <backing-map-scheme>
                <read-write-backing-map-scheme>
                    <internal-cache-scheme>
                        <local-scheme>
                            <scheme-ref>5D-unlimited-backing-map</scheme-ref>
                        </local-scheme>
                    </internal-cache-scheme>
                    <cachestore-scheme>
                        <class-scheme>
                            <class-name>
                                com.oracle.acs.reporting.SubscriptionGroupCacheLoader
                            </class-name>
                            <init-params>
                                <init-param>
                                    <param-type>java.lang.String</param-type>
                                    <param-value>{cache-name}</param-value>
                                </init-param>
                                <init-param>
                                    <param-type>com.tangosol.net.BackingMapManagerContext</param-type>
                                    <param-value>{manager-context}</param-value>
                                </init-param>
                            </init-params>
                        </class-scheme>
                    </cachestore-scheme>
                    <refresh-ahead-factor>0.5</refresh-ahead-factor>
                </read-write-backing-map-scheme>
            </backing-map-scheme>
            <autostart>true</autostart>
        </distributed-scheme>
        <distributed-scheme>
            <scheme-name>subscriptionObjects-distributed</scheme-name>
            <service-name>subscriptionObjects-distributed</service-name>
            <thread-count>5</thread-count>
            <backing-map-scheme>
                <read-write-backing-map-scheme>
                    <internal-cache-scheme>
                        <local-scheme>
                            <scheme-ref>5D-unlimited-backing-map</scheme-ref>
                        </local-scheme>
                    </internal-cache-scheme>
                    <cachestore-scheme>
                        <class-scheme>
                            <class-name>
                                com.oracle.acs.reporting.SubscriptionObjectCacheLoader
                            </class-name>
                            <init-params>
                                <init-param>
                                    <param-type>java.lang.String</param-type>
                                    <param-value>{cache-name}</param-value>
                                </init-param>
                                <init-param>
                                    <param-type>com.tangosol.net.BackingMapManagerContext</param-type>
                                    <param-value>{manager-context}</param-value>
                                </init-param>
                            </init-params>
                        </class-scheme>
                    </cachestore-scheme>
                    <refresh-ahead-factor>0.5</refresh-ahead-factor>
                </read-write-backing-map-scheme>
            </backing-map-scheme>
            <autostart>true</autostart>
        </distributed-scheme>
        
        <!--gateway-->
        <distributed-scheme>
            <scheme-name>noexp-misc-distributed</scheme-name>
            <service-name>noexp-misc-distributed</service-name>
            <thread-count>3</thread-count>
            <backing-map-scheme>
                <local-scheme>
                    <eviction-policy>HYBRID</eviction-policy>
                </local-scheme>
            </backing-map-scheme>
            <autostart>true</autostart>
        </distributed-scheme>
        <distributed-scheme>
            <scheme-name>itsp-exp-distributed</scheme-name>
            <service-name>itsp-exp-distributed</service-name>
            <backing-map-scheme>
                <local-scheme>
                    <eviction-policy>HYBRID</eviction-policy>
                    <expiry-delay>4h</expiry-delay>
                    <!--flush-delay>1m</flush-delay-->
                    <!--expiry-enabled>true</expiry-enabled-->
                </local-scheme>
            </backing-map-scheme>
            <autostart>true</autostart>
        </distributed-scheme>
        <distributed-scheme>
            <scheme-name>GatewayDataCache</scheme-name>
            <service-name>GatewayDataCache</service-name>
            <thread-count>5</thread-count>
            <backing-map-scheme>
                <read-write-backing-map-scheme>
                    <internal-cache-scheme>
                        <local-scheme/>
                    </internal-cache-scheme>
                    <cachestore-scheme>
                        <class-scheme>
                            <class-name>com.sun.itsp.service.gateway.GatewayDataCacheStore</class-name>
                            <!--init-params></init-params-->
                        </class-scheme>
                    </cachestore-scheme>
                </read-write-backing-map-scheme>  
            </backing-map-scheme>
            <autostart>true</autostart>
        </distributed-scheme>
        <distributed-scheme>
            <scheme-name>cmd-response-exp-distributed</scheme-name>
            <service-name>cmd-response-exp-distributed</service-name>
            <thread-count>5</thread-count>
            <backing-map-scheme>
                <read-write-backing-map-scheme>
                    <internal-cache-scheme>
                        <local-scheme>
                             <eviction-policy>HYBRID</eviction-policy>
                            <expiry-delay>8h</expiry-delay>
                            <!--flush-delay>1m</flush-delay-->
                            <!--expiry-enabled>true</expiry-enabled-->
                        </local-scheme>
                    </internal-cache-scheme>
                    <cachestore-scheme>
                        <class-scheme>
                            <class-name>
                                com.oracle.acs.cache.stores.RemoteCommandCacheStore
                            </class-name>
                        </class-scheme>
                    </cachestore-scheme>
                </read-write-backing-map-scheme>
            </backing-map-scheme>
            <autostart>true</autostart>
        </distributed-scheme>
		<distributed-scheme>
			<scheme-name>orgIdsMapping-distributed</scheme-name>
			<service-name>orgIdsMapping-distributed</service-name>
			<thread-count>5</thread-count>
			<backing-map-scheme>
				<read-write-backing-map-scheme>
					<internal-cache-scheme>
					<local-scheme>
						<expiry-delay>7D</expiry-delay>
					</local-scheme>
					</internal-cache-scheme>
					<cachestore-scheme>
						<class-scheme>
							<class-name>
								com.oracle.acs.reporting.OrgIdsMappingStore
							</class-name>
							<init-params>
								<init-param>
									<param-type>java.lang.String</param-type>
									<param-value>{cache-name}</param-value>
								</init-param>
							</init-params>
						</class-scheme>
					</cachestore-scheme>
				</read-write-backing-map-scheme>
			</backing-map-scheme>
			<autostart>true</autostart>
		</distributed-scheme>
        <!--keep-->
        <distributed-scheme>
            <scheme-name>JGCache</scheme-name>
            <service-name>JGCache</service-name>
            <backing-map-scheme>
                <local-scheme>
                    <expiry-delay>1h</expiry-delay>
                </local-scheme>
            </backing-map-scheme>
            <autostart>true</autostart>
        </distributed-scheme>
    </caching-schemes>
</cache-config>
